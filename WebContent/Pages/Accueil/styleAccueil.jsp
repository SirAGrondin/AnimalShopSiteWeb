<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<style type="text/css">
body{
    background : white;
	box-shadow: 1px 1px green;
}

h1{
	text-align: center;
	font-size: 50px;
}

#image{
	width: 100%;
	height: 394px;
	display: block;
}
img{
	margin: 0 auto;
	display: block;
}

header a {
	background: white;
	color: black;
	border-radius: 5px;
	font-size: 30px;
	font-weight: bold;
	padding: 5px;
	box-shadow: 2px 2px 1px black;
	text-decoration: none;
	transition: color 1s ease 0s;
}

header a:hover{
	color: darkgreen;
	text-decoration: underline;
}

header a:active{
	box-shadow: 1px 1px black;
}

#date{
	
}

#heure{
	background: black;
	color: yellow;
	text-align: center;
	border-radius: 5px;
	border-color: yellow;
	border-width: 2px;
	border-style: solid;
	font-size: 20px;
	font-weight: bold;
	padding: 3px;
	display: block;
	width: 10%;
}

#compteur{
	background: black;
	color: white;
	text-align: center;
	border-radius: 5px;
	border-color: white;
	border-width: 2px;
	border-style: solid;
	font-size: 20px;
	font-weight: bold;
	padding: 20px;
	display: block;
	width: 10%;}
}
</style>
</head>
